{
  "name": "fragments",
  "private": true,
  "version": "0.10.5",
  "description": "Fragments back-end API",
  "scripts": {
    "test:watch": "jest -c jest.config.js --runInBand --watch --",
    "test": "jest -c jest.config.js --runInBand --",
    "test:integration": "hurl --test --glob \"tests/integration/**/*.hurl\"",
    "coverage": "jest -c jest.config.js --runInBand --coverage",
    "lint": "eslint --config .eslintrc.js \"./src/**/*.js\" \"tests/**/*.js\"",
    "start": "node src/index.js",
    "dev": "LOG_LEVEL=debug nodemon ./src/index.js --watch src",
    "debug": "LOG_LEVEL=debug nodemon --inspect=0.0.0.0:9229 ./src/index.js --watch src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/avelynhc/fragments.git"
  },
  "author": "Hyunjeong Choi",
  "license": "UNLICENSED",
  "devDependencies": {
    "@orangeopensource/hurl": "^4.0.0",
    "eslint": "^8.40.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "prettier": "2.8.8",
    "supertest": "^6.3.3"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.370.0",
    "@aws-sdk/client-s3": "^3.369.0",
    "@aws-sdk/lib-dynamodb": "^3.370.0",
    "aws-jwt-verify": "^4.0.0",
    "compression": "^1.7.4",
    "content-type": "^1.0.5",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "htpasswd": "^2.4.6",
    "http-auth": "^4.2.0",
    "http-auth-passport": "^1.0.7",
    "markdown-it": "^13.0.1",
    "mime-types": "^2.1.35",
    "passport": "^0.6.0",
    "passport-http-bearer": "^1.0.1",
    "pino": "^8.14.1",
    "pino-http": "^8.3.3",
    "pino-pretty": "^10.0.0",
    "stoppable": "^1.1.0"
  }
}
